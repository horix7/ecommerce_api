{
  "name": "restful-ecommerce",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && babel src -d dist",
    "clean": "rimraf dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db:migrate": "sequelize db:migrate",
    "db:refresh": "npm run db:reset && npm run db:migrate",
    "db:reset": "sequelize db:migrate:undo:all",
    "start:prod": "pm2 start dist/bin/www.js --name ecomerce-api",
    "db:seed": "sequelize db:seed:all",
    "lint": "eslint src",
    "sequelize": "sequelize",
    "serve": "nodemon -r ./config.js --exec babel-node src/bin/www.js",
    "start": "node ./dist/bin/www.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@types/jest": "^25.2.2",
    "babel-jest": "^26.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.8.0",
    "express": "~4.16.1",
    "express-validator": "^6.4.0",
    "faker": "^4.1.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jest": "^26.0.1",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "morgan": "~1.9.1",
    "pg": "^8.2.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.9",
    "supertest": "^4.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
